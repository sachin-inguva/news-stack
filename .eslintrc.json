{
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true
  },
  "parser": "babel-eslint",
  "ignorePatterns": ["/*/**/*.json", "/public/*", "/*/**/*.svg", "/*/**/*.css"],
  "extends": [
    "plugin:json/recommended",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {}
}
